            },
            "EveAllianceSchema": {
                "properties": {
                    "logo_url": {
                        "title": "Logo Url",
                        "type": "string"
                    },
                    "alliance_id": {
                        "title": "Alliance Id",
                        "type": "integer"
                    },
                    "alliance_name": {
                        "maxLength": 254,
                        "title": "Alliance Name",
                        "type": "string"
                    }
                },
                "required": [
                    "logo_url",
                    "alliance_id",
                    "alliance_name"
                ],
                "title": "EveAllianceSchema",
                "type": "object"
            },
            "ContactLabelSchema": {
                "properties": {
