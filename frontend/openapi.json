                    }
                },
                "required": [
                    "label_name"
                ],
                "title": "ContactLabelSchema",
                "type": "object"
            },
            "ContactSchema": {
                "properties": {
                    "id": {
                        "title": "Id",
                        "type": "integer"
                    },
                    "contact_id": {
                        "title": "Contact Id",
                        "type": "integer"
                    },
                    "contact_type": {
                        "$ref": "#/components/schemas/ContactTypeOptions"
                    },
                    "contact_logo_url": {
                        "title": "Contact Logo Url",
                        "type": "string"
                    },
                    "contact_name": {
                        "title": "Contact Name",
                        "type": "string"
                    },
                    "standing": {
                        "title": "Standing",
                        "type": "number"
                    },
                    "notes": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Notes"
                    },
                    "can_edit_notes": {
                        "title": "Can Edit Notes",
                        "type": "boolean"
                    },
                    "labels": {
                        "default": [],
                        "items": {
                            "$ref": "#/components/schemas/ContactLabelSchema"
                        },
                        "title": "Labels",
                        "type": "array"
                    }
                },
                "required": [
                    "id",
                    "contact_id",
                    "contact_type",
                    "contact_logo_url",
                    "contact_name",
                    "standing",
                    "can_edit_notes"
                ],
                "title": "ContactSchema",
                "type": "object"
            },
            "ContactTypeOptions": {
                "enum": [
                    "character",
                    "corporation",
                    "alliance",
                    "faction"
                ],
                "title": "ContactTypeOptions",
                "type": "string"
            },
            "UpdateContactSchema": {
                "properties": {
                    "notes": {
                        "title": "Notes",
                        "type": "string"
                    }
                },
                "required": [
                    "notes"
                ],
                "title": "UpdateContactSchema",
                "type": "object"
            },
            "CorporationTokenSchema": {
                "properties": {
                    "last_update": {
                        "format": "date-time",
                        "title": "Last Update",
                        "type": "string"
                    },
                    "corporation": {
                        "$ref": "#/components/schemas/EveCorporationSchema"
                    }
                },
                "required": [
                    "last_update",
                    "corporation"
                ],
                "title": "CorporationTokenSchema",
                "type": "object"
            },
            "EveCorporationSchema": {
                "properties": {
                    "alliance": {
                        "anyOf": [
                            {
                                "$ref": "#/components/schemas/EveAllianceSchema"
                            },
                            {
                                "type": "null"
                            }
                        ]
                    },
                    "logo_url": {
                        "title": "Logo Url",
                        "type": "string"
                    },
                    "corporation_id": {
                        "title": "Corporation Id",
                        "type": "integer"
                    },
                    "corporation_name": {
                        "maxLength": 254,
                        "title": "Corporation Name",
                        "type": "string"
                    }
                },
                "required": [
                    "logo_url",
                    "corporation_id",
                    "corporation_name"
                ],
                "title": "EveCorporationSchema",
                "type": "object"
            },
            "UserPermissionsSchema": {
                "properties": {
                    "can_manage_alliance_contacts": {
                        "title": "Can Manage Alliance Contacts",
                        "type": "boolean"
                    },
                    "can_manage_corporation_contacts": {
                        "title": "Can Manage Corporation Contacts",
                        "type": "boolean"
                    }
                },
                "required": [
                    "can_manage_alliance_contacts",
                    "can_manage_corporation_contacts"
                ],
                "title": "UserPermissionsSchema",
                "type": "object"
            }
        },
        "securitySchemes": {
            "SessionAuth": {
                "type": "apiKey",
                "in": "cookie",
                "name": "sessionid"
            }
        }
    },
    "servers": []
